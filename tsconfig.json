{
	"compilerOptions": {
		"target": "ES2021",
		"module": "commonjs",
		"moduleResolution": "Node",
		"rootDir": "./src",
		"outDir": "./dist",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"removeComments": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
